        } else if(w_inf[i] <= 0){
                w_inf[i] = 0
        }
}


w_prem = rnorm(10, desc_stat_both$mean[2], desc_stat_both$sd[2])
for(i in 1:length(w_prem)){
        #print(w_inf[i])
